{
  "name": "etymology-map",
  "description": "Mapping English word etymology",
  "homepage": "https://leegee.github.io/etymology-map/",
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "deploy": "bun run build && gh-pages -d dist/",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "start": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "minify": "bun -e 'import fs from \"fs\"; fs.writeFileSync(\"public/countries.min.geojson\", JSON.stringify(JSON.parse(fs.readFileSync(\"data/countries.geojson\"))))'",
    "db:build": "bun tsx scripts/build-db.ts",
    "db:see": "bun tsx scripts/see.ts",
    "db:tsv": "bun tsx scripts//build-tsv-db.ts",
    "build:fallback-map": "tsx scripts/build-fallback-lang-map.tsx"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/maplibre-gl": "^1.14.0",
    "@types/sql.js": "^1.4.9",
    "@vitest/ui": "3.0.5",
    "better-sqlite3": "*",
    "gh-pages": "^6.3.0",
    "jsdom": "^25.0.1",
    "solid-js": "^1.9.8",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "3.0.5"
  },
  "dependencies": {
    "@types/log4js": "^2.3.5",
    "beercss": "^3.11.33",
    "flag-icons": "^7.5.0",
    "iso-639-3": "^3.0.1",
    "log4js": "^6.9.1",
    "maplibre-gl": "^5.7.0",
    "sql.js": "^1.13.0"
  }
}